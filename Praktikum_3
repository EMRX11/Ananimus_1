   "source": [
    "#Javobingizni shu yerga yozing\n",
    "mask=df.groupby(['Regionname']).YearBuilt.mean()\n",
    "for idx in mask.index:\n",
    "    df[df.Regionname==idx]=df[df.Regionname==idx].fillna({\"YearBuilt\":mask[idx]})"
   ]
